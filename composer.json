        "twig/string-extra": "2.12.3",
        "twig/twig": "2.12.3",
        "zircote/swagger-php": "3.0.3"
    },
    "require-dev": {
        "ext-tokenizer": "*",
        "ext-xmlwriter": "*",
        "bheller/images-generator": "1.0.1",
        "fzaninotto/faker": "1.9.1",
        "johnkary/phpunit-speedtrap": "3.1.0",
        "league/flysystem-memory": "1.0.2",
        "mbezhanov/faker-provider-collection": "1.2.1",
        "nikic/php-parser": "4.3.0",
        "opis/json-schema": "1.0.19",
        "phpunit/phpunit": "8.5.2",
        "smalot/pdfparser": "0.14.0",
        "symfony/browser-kit": "~4.4.13",
        "symfony/dependency-injection": "~4.4.13",
        "symfony/dom-crawler": "~4.4.13",
        "symfony/phpunit-bridge": "~4.4.13",
        "symfony/stopwatch": "~4.4.13",
        "symfony/var-dumper": "~4.4.13",
        "symfony/web-profiler-bundle": "~4.4.13"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "autoload": {
        "psr-4": {
            "Shopware\\Administration\\": "src/Administration/",
            "Shopware\\Docs\\": "src/Docs/",
            "Shopware\\Storefront\\": "src/Storefront/",
            "Shopware\\Core\\": "src/Core/",
            "Shopware\\Elasticsearch\\": "src/Elasticsearch/"
        },
        "exclude-from-classmap": [
            "src/**/Test/"
        ]
    }
}
