        "league/construct-finder": "^1.1",
        "nikic/php-parser": "~4.15.0",
        "opis/json-schema": "~2.3.0",
        "phpbench/phpbench": "^1.2",
        "phpdocumentor/reflection-docblock": "~5.3.0",
        "phpdocumentor/type-resolver": "~1.7.1",
        "phpstan/extension-installer": "^1.2.0",
        "phpstan/phpstan": "1.9.14",
        "phpstan/phpstan-deprecation-rules": "1.1.1",
        "phpstan/phpstan-doctrine": "1.3.32",
        "phpstan/phpstan-phpunit": "1.3.3",
        "phpstan/phpstan-symfony": "1.2.20",
        "phpunit/phpunit": "~9.6",
        "smalot/pdfparser": "~2.2.2",
        "symfony/browser-kit": "~6.2.0",
        "symfony/css-selector": "~6.2.0",
        "symfony/dom-crawler": "~6.2.0",
        "symfony/expression-language": "~6.2.0",
        "symfony/phpunit-bridge": "~6.1.0",
        "symfony/var-dumper": "~6.2.0",
        "symplify/phpstan-rules": "11.2.5",
        "shopware/dev-tools": "^1.0.1",
        "tomasvotruba/type-coverage": "^0.1.3"
