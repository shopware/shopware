    "suggest": {
        "shopware/dev-tools": "For development tools, profiler, faker, etc"
    },
    "repositories": [
        {
            "type": "path",
            "url": "custom/plugins/*",
            "options": {
                "symlink": true
            }
        }
    ],
    "conflict": {
        "monolog/monolog": "^1.0",
        "enqueue/enqueue": "<0.10.16"
    },
