{
    "name": "shopware/platform",
    "description": "The Shopware e-commerce core",
    "type": "library",
    "keywords": [
        "framework",
        "shopware"
    ],
    "homepage": "https://www.shopware.com",
    "license": "MIT",
    "replace": {
        "shopware/core": "self.version",
        "shopware/storefront": "self.version",
        "shopware/administration": "self.version",
        "shopware/elasticsearch": "self.version"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.3.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "7.2.9"
        },
        "sort-packages": true
    },
    "require": {
        "php": ">=7.2.0",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-simplexml": "*",
        "ext-xml": "*",
        "ext-zip": "*",
        "aws/aws-sdk-php": "3.133.6",
        "cocur/slugify": "4.0.0",
        "composer/composer": "1.10.15",
        "composer/package-versions-deprecated": "^1.8.0",
        "composer/semver": "1.7.1",
        "doctrine/annotations": "1.8.0",
        "doctrine/dbal": "2.10.1",
        "doctrine/inflector": "1.3.1",
        "dompdf/dompdf": "0.8.4",
        "elasticsearch/elasticsearch": "7.2.2",
        "enqueue/dbal": "0.10.0",
        "erusev/parsedown-extra": "0.8.1",
        "ezyang/htmlpurifier": "4.13.0",
        "google/cloud-storage": "1.17.0",
        "guzzlehttp/guzzle": "6.5.2",
        "jdorn/sql-formatter": "1.2.17",
        "lcobucci/jwt": "3.3.1",
        "league/flysystem": "1.0.63",
        "league/flysystem-aws-s3-v3": "1.0.23",
        "league/oauth2-server": "7.3.3",
        "marc1706/fast-image-size": "1.1.6",
        "nyholm/psr7": "1.2.1",
        "ongr/elasticsearch-dsl": "7.1.3",
        "padaliyajay/php-autoprefixer": "1.2",
        "psr/cache": "1.0.1",
        "psr/event-dispatcher": "1.0.0",
        "psr/http-factory": "1.0.1",
        "psr/http-message": "1.0.1",
        "psr/log": "1.1.2",
        "scssphp/scssphp": "1.0.6",
        "sensio/framework-extra-bundle": "5.5.3",
        "sroze/messenger-enqueue-transport": "0.4.0",
        "superbalist/flysystem-google-storage": "7.2.2",
