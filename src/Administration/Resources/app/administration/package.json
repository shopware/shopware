{
    "name": "administration",
    "version": "1.0.0",
    "description": "Shopware administration",
    "contributors": [
        {
            "name": "Stephan Pohl",
            "email": "s.pohl@shopware.com"
        },
        {
            "name": "Philipp Schuch",
            "email": "p.schuch@shopware.com"
        }
    ],
    "scripts": {
        "dev": "node build/dev-server.js",
        "build": "node build/build.js",
        "unit": "jest --config jest.config.js --ci",
        "unit-watch": "jest --config jest.config.js --watch",
        "test-debug": "node --inspect node_modules/.bin/jest --watch --config jest.config.js --runInBand --no-cache",
        "lint": "eslint --ext .js,.vue src",
        "lint:scss": "LIST=`git diff --name-only --diff-filter=ACMR HEAD --relative | grep -E '\\.(scss)'`; if [ \"$LIST\" ]; then stylelint $LIST; fi",
        "lint:scss:fix": "LIST=`git diff --name-only --diff-filter=ACMR HEAD --relative | grep -E '\\.(scss)'`; if [ \"$LIST\" ]; then stylelint $LIST --fix; fi",
        "lint:scss-all": "stylelint '**/*.scss'",
        "lint:scss-all:fix": "stylelint '**/*.scss' --fix",
        "security-check": "./node_modules/.bin/nsp check",
        "generate-api-docs": "./node_modules/.bin/jsdoc src -r -c jsdoc.config.js"
    },
    "dependencies": {
        "@babel/runtime": "7.5.5",
        "@shopware-ag/eslint-config-base": "1.0.0",
        "@shopware-ag/webpack-copy-after-build": "1.0.1",
