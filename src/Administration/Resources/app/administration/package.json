    "lint": "eslint --ext .js,.ts,.vue,.html.twig src test",
    "lint:fix": "npm run lint -- --fix",
    "lint:types": "tsc",
    "lint:twig": "eslint --ext .html.twig src",
    "lint:junit": "eslint --ext .js,.ts,.vue,.html.twig --format junit src test > eslint.junit.xml",
