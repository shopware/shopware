  "scripts": {
    "postinstall": "patch-package",
    "dev": "mode=development webpack-dev-server",
    "build": "mode=production webpack",
    "unit": "jest --config jest.config.js --ci",
    "unit-watch": "jest --config jest.config.js --watch",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --watch --config jest.config.js --runInBand --no-cache",
    "lint": "eslint --ext .js,.vue,.html.twig src test",
    "lint:twig": "eslint --ext .html.twig src",
    "lint:junit": "eslint --ext .js,.vue,.html.twig --format junit src test > eslint.junit.xml",
    "lint-fix": "eslint --fix --ext .js,.vue,.html.twig src test",
    "lint:scss": "stylelint **/*.scss",
    "lint:scss-fix": "npm run lint:scss -- --fix",
    "security-check": "./node_modules/.bin/nsp check",
    "generate-api-docs": "./node_modules/.bin/jsdoc src -r -c jsdoc.config.js"
  },
