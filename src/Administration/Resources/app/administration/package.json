        "bottlejs": "1.7.2",
        "cookie-storage": "^6.0.0",
        "copy-webpack-plugin": "5.1.1",
        "dompurify": "2.0.7",
        "eslint-friendly-formatter": "4.0.1",
        "flatpickr": "4.6.3",
        "lodash": "4.17.15",
        "md5-es": "1.8.2",
        "mini-css-extract-plugin": "0.8.0",
