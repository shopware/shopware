<!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
{% block sw_meteor_navigation %}
<div
    v-if="hasParentRoute"
    class="sw-meteor-navigation"
>
    <!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
    {% block sw_meteor_navigation_link %}
    <router-link
        :to="{ name: parentRoute.name }"
        class="sw-meteor-navigation__link"
    >
        <!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
        {% block sw_meteor_navigation_link_icon %}
        <sw-icon
            class="sw-meteor-navigation__back-arrow"
            name="regular-long-arrow-left"
            size="14px"
        />
        {% endblock %}

        <!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
        {% block sw_meteor_navigation_link_label %}
        {{ parentRoute.label }}
        {% endblock %}
    </router-link>
    {% endblock %}
</div>
{% endblock %}
