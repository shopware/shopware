{% block sw_condition_value_content %}
<div class="sw-condition-nested-rule sw-condition__condition-value">
    {% block sw_condition_nested_rule_field_operator %}
    <sw-condition-operator-select
        v-bind="{ operators, condition }"
        :disabled="disabled"
    />
    {% endblock %}

    {% block sw_condition_nested_rule_field_rule_ids %}
    <sw-entity-single-select
        ref="ruleId"
        v-model="ruleId"
        size="medium"
        entity="rule"
        :disabled="disabled"
    />
    {% endblock %}
</div>
{% endblock %}
