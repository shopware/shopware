{% block sw_condition_value_content %}
<div class="sw-condition-sales-channel sw-condition__condition-value">
    {% block sw_condition_sales_channel_field_operator %}
    <sw-condition-operator-select
        v-bind="{ operators, condition }"
        :disabled="disabled"
    />
    {% endblock %}

    {% block sw_condition_sales_channel_field_sales_channel_ids %}
    <sw-entity-multi-select
        size="medium"
        entity="sales_channel"
        :disabled="disabled"
        :entity-collection="salesChannels"
        @change="setSalesChannelIds"
    />
    {% endblock %}
</div>
{% endblock %}
