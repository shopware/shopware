        {% block sw_condition_shipping_street_field_operator %}
            <sw-condition-operator-select v-bind="{ operators, condition }"></sw-condition-operator-select>
        {% endblock %}

        {% block sw_condition_shipping_street_field_street_name %}
            <sw-text-field size="medium" v-model="streetName"></sw-text-field>
        {% endblock %}
    </div>
{% endblock %}
