<!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
{% block sw_search_more_results %}
<router-link
    v-if="searchTypeRoute"
    :to="{ name: searchTypeRoute, query: { term: term }}"
    class="sw-search-more-results__link sw-search-more-results__link--v2"
>
    <!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
    {% block sw_search_more_results_content %}
    <slot name="content">
        <!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
        {% block sw_search_more_results_slot_content %} {{ searchContent }} {% endblock %}
    </slot>
    {% endblock %}
</router-link>
{% endblock %}
