{% block sw_progress_bar %}
    <div class="sw-progress-bar">
        {% block sw_progress_bar_total %}
            <div class="sw-progress-bar__total">
                {% block sw_progress_bar_total_value %}
                    <div class="sw-progress-bar__value" :style="{ width: styleWidth }" :class="progressClasses"></div>
                {% endblock %}
            </div>
        {% endblock %}
    </div>
{% endblock %}