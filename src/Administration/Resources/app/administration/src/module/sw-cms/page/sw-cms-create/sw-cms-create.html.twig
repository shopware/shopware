<!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
{% block sw_cms_detail_toolbar_language_switch %}
<sw-language-switch
    slot="language-switch"
    disabled
/>
{% endblock %}

<!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
{% block sw_cms_detail_toolbar_actions_sales_channel %}
{% endblock %}

<!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
{% block sw_cms_detail_toolbar_actions_save %}
<sw-button-process
    v-tooltip.bottom="tooltipSave"
    class="sw-cms-detail__save-action"
    :is-loading="isLoading"
    :process-success="isSaveSuccessful"
    :disabled="true"
    variant="primary"
>
    {{ $tc('sw-cms.detail.label.buttonSave') }}
</sw-button-process>
{% endblock %}

<!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
{% block sw_cms_detail_stage_container %}
<sw-cms-create-wizard
    :page="page"
    @on-section-select="addFirstSection($event, 0)"
    @wizard-complete="onWizardComplete"
/>
{% endblock %}
