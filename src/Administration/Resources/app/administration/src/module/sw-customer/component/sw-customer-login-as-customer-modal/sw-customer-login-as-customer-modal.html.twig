<!-- eslint-disable vuejs-accessibility/click-events-have-key-events -->
<!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
{% block sw_customer_login_as_customer_modal %}
<sw-modal
    :title="modalTitle"
    class="sw-customer-login-as-customer-modal"
    @modal-close="onCancel"
>
    <!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
        {% block sw_customer_login_as_customer_modal_body %}
    <sw-container
        columns="1fr 1fr"
        gap="0px 14px"
    >
        <!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
                {% block sw_customer_login_as_customer_modal_sales_channel_domain_items %}
        <sw-context-menu-item
            v-for="salesChannelDomain of salesChannelDomains"
            :key="salesChannelDomain.id"
            icon="default-action-external"
            target="_blank"
            @click="onSalesChannelDomainMenuItemClick(salesChannelDomain.salesChannelId, salesChannelDomain.url)">
            {{ salesChannelDomain.url }}
        </sw-context-menu-item>
                {% endblock %}
    </sw-container>
        {% endblock %}

    <!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
        {% block sw_customer_login_as_customer_modal_footer %}
    <template #modal-footer>
        <!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
        <div>
            <!-- eslint-disable-next-line sw-deprecation-rules/no-twigjs-blocks -->
                    {% block sw_customer_login_as_customer_modal_footer_cancel %}
            <sw-button
                size="small"
                @click="onCancel"
            >
                {{ $tc('sw-customer.loginAsCustomerModal.labelCancel') }}
            </sw-button>
                    {% endblock %}
        </div>
    </template>
        {% endblock %}
</sw-modal>
{% endblock %}
