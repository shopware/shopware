{
    "name": "shopware/administration",
    "description": "Administration frontend for the Shopware Core",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shopware\\Administration\\": ""
        }
    },
    "config": {
        "platform": {
            "php": "7.4.3"
        },
        "sort-packages": true
    },
    "support": {
        "issues": "https://issues.shopware.com",
        "forum": "https://forum.shopware.com",
        "wiki": "https://developer.shopware.com",
        "docs": "https://developer.shopware.com",
        "chat": "https://slack.shopware.com"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.4.x-dev",
            "dev-trunk": "6.4.x-dev"
        }
    },
    "require": {
        "php": "^7.4.3 || ^8.0",
        "shopware/core": "*",
        "symfony/framework-bundle": "~5.3.0",
        "symfony/http-foundation": "~5.3.0",
        "symfony/routing": "~5.3.0",
        "symfony/mime": "~5.3.0"
    }
}
