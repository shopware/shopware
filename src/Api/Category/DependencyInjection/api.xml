<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
         <service class="Shopware\Api\Category\Definition\CategoryDefinition" id="shopware.category.category_definition" >
            <tag name="shopware.entity.definition" entity="category" />
        </service>
        <service class="Shopware\Api\Category\Repository\CategoryRepository" id="Shopware\Api\Category\Repository\CategoryRepository" public="true">
          <argument id="shopware.api.entity_reader" type="service"/>
          <argument id="shopware.api.entity_writer" type="service"/>
          <argument id="shopware.api.entity_searcher" type="service"/>
          <argument id="shopware.api.entity_aggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
         <service class="Shopware\Api\Category\Definition\CategoryTranslationDefinition" id="shopware.category.category_translation_definition" >
            <tag name="shopware.entity.definition" entity="category_translation" />
        </service>
        <service class="Shopware\Api\Category\Repository\CategoryTranslationRepository" id="Shopware\Api\Category\Repository\CategoryTranslationRepository" public="true">
          <argument id="shopware.api.entity_reader" type="service"/>
          <argument id="shopware.api.entity_writer" type="service"/>
          <argument id="shopware.api.entity_searcher" type="service"/>
          <argument id="shopware.api.entity_aggregator" type="service"/>
          <argument id="event_dispatcher" type="service"/>
        </service>
    </services>
</container>                
                