                    "deliveryDate": {
                        "type": "object",
                        "properties": {
                            "earliest": {
                                "type": "string",
                                "format": "date-time"
                            },
                            "latest": {
                                "type": "string",
                                "format": "date-time"
                            }
                        }
                    },
                    "location": {
                        "type": "object",
                        "properties": {
                            "apiAlias": {
