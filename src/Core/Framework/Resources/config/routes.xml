<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing
        https://symfony.com/schema/routing/routing-1.0.xsd">

    <import resource="../../Api/Controller/**/*Controller.php" type="annotation" />
    <import resource="../../Plugin/**/*Controller.php" type="annotation" />
    <import resource="../../Store/**/*Controller.php" type="annotation" />
    <import resource="../../Update/**/*Controller.php" type="annotation" />
    <import resource="../../MessageQueue/**/*Controller.php" type="annotation" />
    <import resource="../../Increment/Controller/*Controller.php" type="annotation" />
    <import resource="../../Migration/**/*Controller.php" type="annotation" />
    <import resource="../../App/**/*Controller.php" type="annotation" />
    <import resource="../../Script/Api/*Route.php" type="annotation" />
    <import resource="../../Rule/Api/*Controller.php" type="annotation" />

</routes>
