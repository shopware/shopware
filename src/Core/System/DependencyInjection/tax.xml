<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="Shopware\Core\System\Tax\TaxDefinition">
            <tag name="shopware.entity.definition" entity="tax"/>
        </service>

        <service id="Shopware\Core\System\Tax\Aggregate\TaxRule\TaxRuleDefinition">
            <tag name="shopware.entity.definition" entity="tax_rule"/>
        </service>

        <service id="Shopware\Core\System\Tax\Aggregate\TaxRuleType\TaxRuleTypeDefinition">
            <tag name="shopware.entity.definition" entity="tax_rule_type"/>
        </service>

        <service id="Shopware\Core\System\Tax\Aggregate\TaxRuleTypeTranslation\TaxRuleTypeTranslationDefinition">
            <tag name="shopware.entity.definition" entity="tax_rule_type_translation"/>
        </service>

        <service id="Shopware\Core\System\Tax\TaxRuleType\EntireCountryRuleTypeFilter">
            <tag name="tax.rule_type_filter"/>
        </service>

        <service id="Shopware\Core\System\Tax\TaxRuleType\IndividualStatesRuleTypeFilter">
            <tag name="tax.rule_type_filter"/>
        </service>

        <service id="Shopware\Core\System\Tax\TaxRuleType\ZipCodeRangeRuleTypeFilter">
            <tag name="tax.rule_type_filter"/>
        </service>

        <service id="Shopware\Core\System\Tax\TaxRuleType\ZipCodeRuleTypeFilter">
            <tag name="tax.rule_type_filter"/>
        </service>
    </services>
</container>
