{
    "name": "shopware/docs",
    "description": "Docs for the Shopware Core",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shopware\\Docs\\": ""
        },
        "exclude-from-classmap": [
            "*/Test/"
        ]
    },
    "config": {
        "platform": {
            "php": "7.2.9"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.3.x-dev"
        }
    },
    "require": {
        "cocur/slugify": "4.0.0",
        "erusev/parsedown-extra": "0.8.1",
        "guzzlehttp/guzzle": "6.5.2",
        "shopware/core": "*",
        "symfony/config": "~4.4.13",
        "symfony/console": "~4.4.13",
        "symfony/dependency-injection": "~4.4.13",
        "symfony/filesystem": "~4.4.13",
        "symfony/finder": "~4.4.13",
        "symfony/http-kernel": "~4.4.13"
    }
}
