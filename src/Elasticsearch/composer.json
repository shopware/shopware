{
    "name": "shopware/elasticsearch",
    "description": "Elasticsearch for Shopware",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shopware\\Elasticsearch\\": ""
        },
        "exclude-from-classmap": [
            "/Test/"
        ]
    },
    "config": {
        "platform": {
            "php": "7.4.3"
        },
        "sort-packages": true
    },
    "support": {
        "issues": "https://issues.shopware.com",
        "forum": "https://forum.shopware.com",
        "wiki": "https://developer.shopware.com",
        "docs": "https://developer.shopware.com",
        "chat": "https://slack.shopware.com"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.4.x-dev",
            "dev-trunk": "6.4.x-dev"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4.3 || ^8.0",
        "ext-curl": "*",
        "doctrine/dbal": "~2.13.8",
        "elasticsearch/elasticsearch": "7.16.0",
        "ongr/elasticsearch-dsl": "7.2.2",
        "shopware/core": "*",
        "symfony/http-foundation": "~5.4.1",
        "symfony/messenger": "~5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5.17"
    }
}
