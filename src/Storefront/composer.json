{
    "name": "shopware/storefront",
    "description": "Storefront for Shopware",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shopware\\Storefront\\": ""
        },
        "exclude-from-classmap": [
            "*/Test/"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "support": {
        "issues": "https://issues.shopware.com",
        "forum": "https://forum.shopware.com",
        "wiki": "https://developer.shopware.com",
        "docs": "https://developer.shopware.com",
        "chat": "https://slack.shopware.com"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.5.x-dev",
            "dev-trunk": "6.5.x-dev"
        }
    },
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "cocur/slugify": "^4.3.0",
        "doctrine/dbal": "^3.6",
        "meyfa/php-svg": "~0.13.0",
        "padaliyajay/php-autoprefixer": "1.4",
        "scssphp/scssphp": "v1.11.0",
        "shopware/core": "*",
        "symfony/cache": "~6.3.0",
        "symfony/cache-contracts": "~3.1.0",
        "symfony/console": "~6.3.0",
        "symfony/config": "~6.3.0",
        "symfony/dependency-injection": "~6.3.0",
        "symfony/event-dispatcher": "~6.3.0",
        "symfony/event-dispatcher-contracts": "~3.1.0",
        "symfony/framework-bundle": "~6.3.0",
        "symfony/http-foundation": "~6.3.0",
        "symfony/http-kernel": "~6.3.0",
        "symfony/routing": "~6.3.0",
        "symfony/service-contracts": "~3.1.0",
        "symfony/validator": "~6.3.0",
        "symfony/security-core": "~6.3.0",
        "symfony/mime": "~6.3.0",
        "twig/intl-extra": "^3.4",
        "twig/string-extra": "^3.4",
        "twig/twig": "~3.4.3"
    },
    "require-dev": {
        "symfony/css-selector": "~6.3.0",
        "phpunit/phpunit": "~9.6"
    }
}
